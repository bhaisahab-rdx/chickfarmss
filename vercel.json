{
  "version": 2,
  "builds": [
    {
      "src": "api/consolidated.js",
      "use": "@vercel/node",
      "config": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    {
      "src": "dist/public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/auth/(.*)", "dest": "/api/consolidated.js" },
    { "src": "/api/user", "dest": "/api/consolidated.js" },
    { "src": "/api/login", "dest": "/api/consolidated.js" },
    { "src": "/api/register", "dest": "/api/consolidated.js" },
    { "src": "/api/logout", "dest": "/api/consolidated.js" },
    { "src": "/api/(.*)", "dest": "/api/consolidated.js" },
    { "src": "/health", "dest": "/api/consolidated.js" },
    { "src": "/health.html", "dest": "/public/health.html" },
    { "src": "/vercel-test.html", "dest": "/public/vercel-test.html" },
    { "src": "/game", "dest": "/public/game.html" },
    { "src": "/game.html", "dest": "/public/game.html" },
    { "src": "/login", "dest": "/public/login.html" },
    { "src": "/login.html", "dest": "/public/login.html" },
    { "src": "/register", "dest": "/public/register.html" },
    { "src": "/register.html", "dest": "/public/register.html" },
    { "src": "/dist/public/assets/(.*)", "dest": "/dist/public/assets/$1" },
    { "src": "/assets/(.*)", "dest": "/dist/public/assets/$1" },
    { "src": "/dist/public/index.html", "dest": "/dist/public/index.html" },
    { "src": "^/$", "dest": "/public/index.html" },
    { "src": "/index.html", "dest": "/public/index.html" },
    { "src": "/(.*\\.(js|css|png|jpg|gif|svg|ico|json|ttf|otf|woff|woff2))$", "dest": "/dist/public/$1" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ],
  "env": {
    "NODE_ENV": "production",
